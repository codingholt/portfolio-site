generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model projects {
  id          Int     @id @default(autoincrement())
  name        String? @db.VarChar(255)
  description String? @db.MediumText
  link        String? @db.VarChar(255)
  image       String? @db.MediumText
}
